package excersises;

public class Chapter8Arrays {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int [] values = {23, 6, 47, 35, 2, 14};
		int total = 0;
		for(int i = 0; i<6; i = i +1)
		{
			total = total + values[i];
			
		}
		System.out.println("The total is " + total/6);
	}

}
